{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "FeatureStore",
  "description": "FeatureStore configuration.",
  "default": {},
  "required": [
    "store"
  ],
  "additionalProperties": true,
  "properties": {
    "store": {
      "type": "object",
      "title": "Store",
      "description": "Information about the store file system.",
      "default": {},
      "required": [
        "root_path"
      ],
      "properties": {
        "root_path": {
          "type": "string",
          "title": "root_path",
          "description": "Root path of the store.",
          "default": "./fstore"
        },
        "git": {
          "type": "object",
          "title": "git",
          "description": "Git confirguration.",
          "default": {}
        },
        "gcs": {
          "type": "object",
          "title": "gcs",
          "description": "GCS configuration.",
          "default": {}
        },
        "s3": {
          "type": "object",
          "title": "s3",
          "description": "S3 configuration.",
          "default": {}
        }
      }
    },
    "feature_collections": {
      "type": "array",
      "title": "feature_collections",
      "description": "Array of FeatureCollections with Features that contain only index properties.",
      "default": []
    }
  }
}